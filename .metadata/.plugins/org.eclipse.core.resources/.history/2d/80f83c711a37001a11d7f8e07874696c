package com.example.demo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

import com.example.demo.util.CustomRepository;

@EnableJpaRepositories(repositoryBaseClass = CustomRepository.class)
@SpringBootApplication
public class EscuelaConfigClientApplication {

	public static void main(String[] args) {
		SpringApplication.run(EscuelaConfigClientApplication.class, args);
	}

}
